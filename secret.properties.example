# Secret Properties Template
# Copy this file to secret.properties and fill in your values
# DO NOT commit secret.properties to git!

# JWT Configuration
access_token.secret=your-secret-key-here-minimum-256-bits
access_token.expire=PT15m

refresh_token.secret=your-refresh-secret-key-here
refresh_token.expire=PT30d

# Internal API Key (must be same for apigateway and identify)
internal.api.key=your-internal-api-key-here

# Database Configuration (for identify service only)
host=localhost
postgres.username=postgres
postgres.image=postgres
postgres.password=your-database-password-here
postgres.port=5432
